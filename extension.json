{
	"name": "NetworkNotice",
	"version": "1.1",
	"author": [
		"Tephus",
		"[http://fo-nttax.de Alex Winkler]"
	],
	"url": "http://liquipedia.net/",
	"descriptionmsg": "networknotice-desc",
