{
	"name": "NetworkNotice",
	"version": "1.2.0",
	"author": [
		"Tephus",
		"[https://fo-nttax.de Alex Winkler]"
	],
	"url": "https://liquipedia.net/",
	"descriptionmsg": "networknotice-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.28.0"
	},
	"MessagesDirs": {
		"NetworkNotice": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"NetworkNoticeAlias": "i18n/NetworkNoticeAlias.php"
	},
	"AutoloadClasses": {
		"Liquipedia\\NetworkNotice\\Colors": "NetworkNoticeColors.php",
		"Liquipedia\\NetworkNotice\\Hooks": "NetworkNoticeHooks.php",
		"Liquipedia\\NetworkNotice\\SpecialNetworkNotice": "SpecialNetworkNotice.php"
	},
	"Hooks": {
		"LiquiFlowNetworkNotice": [
			"Liquipedia\\NetworkNotice\\Hooks::onLiquiFlowNetworkNotice"
		]
	},
	"SpecialPages": {
		"NetworkNotice": "Liquipedia\\NetworkNotice\\SpecialNetworkNotice"
	},
	"ResourceModules": {
		"ext.networknotice.SpecialPage": {
			"styles": [
				"ext.networknotice.SpecialPage.css"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "NetworkNotice/resources"
	},
	"GroupPermissions": {
		"sysop": {
			"usenetworknotice": true
		}
	},
	"AvailableRights": [
		"usenetworknotice"
	],
	"manifest_version": 1
}
