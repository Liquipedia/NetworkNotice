{
	"name": "NetworkNotice",
	"version": "2.0.0",
	"author": [
		"Tephus",
		"[https://fo-nttax.de Alex Winkler]"
	],
	"url": "https://liquipedia.net/",
	"descriptionmsg": "networknotice-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"MessagesDirs": {
		"NetworkNotice": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"NetworkNoticeAlias": "i18n/NetworkNoticeAlias.php"
	},
	"AutoloadNamespaces": {
		"Liquipedia\\NetworkNotice\\": "src/"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"Liquipedia\\NetworkNotice\\Hooks::onBeforePageDisplay"
		],
		"SiteNoticeAfter": [
			"Liquipedia\\NetworkNotice\\Hooks::onSiteNoticeAfter"
		]
	},
	"SpecialPages": {
		"NetworkNotice": "Liquipedia\\NetworkNotice\\SpecialNetworkNotice"
	},
	"ResourceModules": {
		"ext.networknotice.SpecialPage": {
			"styles": [
				"ext.networknotice.SpecialPage.css"
			],
			"position": "bottom"
		},
		"ext.networknotice.Notice": {
			"styles": [
				"ext.networknotice.Notice.css"
			],
			"position": "bottom"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "NetworkNotice/resources"
	},
	"GroupPermissions": {
		"sysop": {
			"usenetworknotice": true
		}
	},
	"AvailableRights": [
		"usenetworknotice"
	],
	"manifest_version": 1
}
