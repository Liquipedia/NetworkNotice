	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "NetworkNotice"
	},
	"GroupPermissions": {
		"sysop": {
			"usenetworknotice": true
		}
	},
	"AvailableRights": [
		"usenetworknotice"
	],
	"manifest_version": 1
